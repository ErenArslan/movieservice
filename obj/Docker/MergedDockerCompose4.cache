services:
  elasticsearch-:
    cap_add:
    - IPC_LOCK
    container_name: elasticsearch
    environment:
      ES_JAVA_OPTS: -Xms512m -Xmx512m
      bootstrap.memory_lock: "true"
      discovery.type: single-node
      xpack.security.enabled: "false"
    image: docker.elastic.co/elasticsearch/elasticsearch:7.7.0
    ports:
    - published: 9200
      target: 9200
    - published: 9300
      target: 9300
    ulimits:
      memlock:
        hard: -1
        soft: -1
      nofile:
        hard: 65536
        soft: 65536
    volumes:
    - elasticsearch-data:/usr/share/elasticsearch/data:rw
  mongodb:
    container_name: mongodb
    image: mongo:latest
    ports:
    - published: 27017
      target: 27017
    volumes:
    - mongodb-data:/data/db:rw
  movieservice.api:
    build:
      context: C:\Users\SingleLanguage\source\repos\AtrosMovie
      dockerfile: MovieService/MovieService.Api/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: movieserviceapi
    ports:
    - published: 8000
      target: 80
    - target: 80
    - target: 443
    volumes:
    - C:\Users\SingleLanguage\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\SingleLanguage\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
  rabbitmq:
    container_name: rabbitmq
    image: rabbitmq:3-management
    ports:
    - published: 15672
      target: 15672
  redisdb:
    container_name: redisdb
    image: redis:alpine
    ports:
    - published: 6379
      target: 6379
version: '3.4'
volumes:
  elasticsearch-data:
    external: false
  mongodb-data:
    external: false